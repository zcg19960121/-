<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.lut.ae.model.BrowserDimension">
	<resultMap type="cn.lut.ae.model.BrowserDimension" id="browserMapper">
		<id column="id" property="id" />
		<result column="browser_name" property="browser" />
	</resultMap>

	<select id="getBrowserDimension" parameterType="cn.lut.ae.model.BrowserDimension"
		resultMap="browserMapper">
		SELECT id,browser_name FROM dimension_browser
		<where>
			<if test="id != 0">
				id = #{id}
			</if>
			<if test="browser != null and browser != ''">
				AND browser_name = #{browser}
			</if>
			<if test="browser_version != null and browser_version != ''">
				AND browser_version = #{browser_version}
			</if>

		</where>
	</select>
</mapper>