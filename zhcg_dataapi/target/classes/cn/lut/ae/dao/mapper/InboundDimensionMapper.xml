<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.lut.ae.model.InboundDimension">
	<resultMap type="cn.lut.ae.model.InboundDimension" id="inboundMapper">
		<id column="id" property="id" />
		<result column="name" property="name" />
	</resultMap>

	<select id="getInboundDimension" parameterType="cn.lut.ae.model.InboundDimension"
		resultMap="inboundMapper">
		SELECT id,name
		FROM dimension_inbound
		<where>
			<if test="id != 0">
				id = #{id}
			</if>
			<if test="name != null and name != ''">
				AND name = #{name}
			</if>
		</where>
	</select>
</mapper>